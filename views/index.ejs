<%- include('partials/_header.ejs') -%>

<body>

  <!-- Navigation -->
  <%- include('partials/_navigation.ejs') -%>

  <!-- Page Header -->
  <header class="masthead" style="background-image: url('img/home-bg.jpg')">
    <div class="overlay"></div>
    <div class="container">
      <div class="row">
        <div class="col-lg-8 col-md-10 mx-auto">
          <div class="site-heading">
            <h1>Clean Blog</h1>
            <span class="subheading">A Blog Theme by Start Bootstrap</span>
          </div>
        </div>
      </div>
    </div>
  </header>

  <!-- Main Content -->
  <div class="container">
    <div class="row">

      <% for (let i =0; i < photos.length; i++) { %>
      <div class="col-lg-8 col-md-10 mx-auto">
        <div class="post-preview">
          <a href="post.html">
            <h2 class="post-title">  <%= photos[i].title %> </h2>
            <h5 class="post-subtitle"> <%= photos[i].detail %> </h5>      
           <td>
            <!--class="post-subtitle" > <%= photos[i].image %> </h8> --><h8  
          </td>
          <p class="post-meta">Post Time :<b> <%= photos[i].dateCreated %></p></b></p>
        </div>
        <hr>
        
       
        
        <!-- Pager -->
        <div class="clearfix">
          <a class="btn btn-primary float-right" href="#">Older Posts &rarr;</a>
        </div>
      </div>

      <% } %>
    </div>

  </div>

  <hr>

  <!-- Footer -->
  <%- include('partials/_footer.ejs') -%>


  <!-- Bootstrap core JavaScript -->
  <script src="vendor/jquery/jquery.min.js"></script>
  <script src="vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

  <!-- Custom scripts for this template -->
  <script src="js/clean-blog.min.js"></script>

</body>

</html>
